#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    float a;
    int b,c,d;
    scanf("%f\n%d\n%d\n%d",&a,&b,&c,&d);
    int q=floor(a);
    int w=ceil(a);
    int e=sqrt(b);
    int r=pow(c,d);
    printf("%d\n%d\n%d\n%d",q,w,e,r);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    return 0;
}
