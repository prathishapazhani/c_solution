#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
    int n, sum;
    scanf("%d", &n);
    if(n < 10 || n > 99) {
        printf("Invalid Input\n");
    } else {
        sum = (n / 10) + (n % 10);
        printf("Sum of Digit %d is %03d\n", n, sum);
    }
    return 0;
}

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    

