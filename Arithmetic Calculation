#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int a, b;

   
    scanf("%d", &a);
    scanf("%d", &b);

   
    printf("Addition=%d\n", a + b);
    printf("Subtraction=%d\n", a - b);
    printf("Multiplication=%d\n", a * b);
    printf("Division=%d\n", a / b);  // Assumes b != 0
    printf("Modulo=%d\n", a % b);    // Assumes b != 0

    




    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
