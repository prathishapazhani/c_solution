#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    
    int n, i;
    scanf("%d", &n);
    int num = 6;
    for(i = 1; i <= n; i++) {
        printf("%d", num);
        if(i != n) printf(" ");
        num += 2 * i + 1;
    }
    return 0;
}

    
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
   
