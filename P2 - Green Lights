#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>



int main() {
    float s, d, t;
    scanf("%f", &s);
    scanf("%f", &d);
    scanf("%f", &t);

    float time_to_reach = (d / s) * 3600;

    if (time_to_reach < t)
        printf("Yes\n");
    else
        printf("No\n");

    return 0;
}
