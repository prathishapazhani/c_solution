 #include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d", &n);
    int arr[n], leaders[n], count = 0;
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    int max = -1;
    for(int i = n - 1; i >= 0; i--) {
        if(arr[i] > max) {
            leaders[count++] = arr[i];
            max = arr[i];
        }
    }

    printf("The Best Leaders are ");
    for(int i = 0; i <= count-1; i++) {
        printf("%d ", leaders[i]);
    }
    return 0;
}

