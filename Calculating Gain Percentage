#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    float A, B, C;
    scanf("%f", &A);
    scanf("%f", &B);
    scanf("%f", &C);

    float cost = A + B;
    float gain = C - cost;
    float gain_percent = (gain / cost) * 100;

    printf("%.2f\n", gain_percent);

    return 0;
}


    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
 
