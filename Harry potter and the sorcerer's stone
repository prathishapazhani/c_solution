#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    int num;
    scanf("%d", &num);
    if (num < 0) num = -num;
    int first = num / 1000;
    int last = num % 10;
    int result = first + last;
    printf("%d", result);
    return 0;
}
