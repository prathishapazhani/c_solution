#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    
    int n, i;
    long long product = 1;

    scanf("%d", &n);

    for(i = 1; i <= n; i++) {
        product *= i;
    }

    printf("Product of value is %lld\n", product);

    return 0;
}


    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
